<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xiao.rxjava2test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.ObserverTypeActivity" />
        <activity android:name=".ui.observer.SimpleActivity" />
        <activity android:name=".ui.observer.SingleObserverExActivity" />
        <activity android:name=".ui.observer.CompletableObserverExActivity" />
        <activity android:name=".ui.observer.FlowableExActivity" />
        <activity android:name=".ui.observer.MaybeObserverExActivity" />

        <activity android:name=".ui.OperatorsActivity" />
