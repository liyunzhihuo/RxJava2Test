        <activity android:name=".ui.operators.ZipExActivity" />
        <activity android:name=".ui.operators.DisposableExActivity" />
        <activity android:name=".ui.operators.TakeExActivity" />
        <activity android:name=".ui.operators.TimerExActivity" />
        <activity android:name=".ui.operators.IntervalExActivity" />
        <activity android:name=".ui.operators.ReduceExActivity" />
        <activity android:name=".ui.operators.BufferExActivity" />
        <activity android:name=".ui.operators.FilterExActivity" />
        <activity android:name=".ui.operators.SkipExActivity" />
        <activity android:name=".ui.operators.ScanExActivity" />
        <activity android:name=".ui.operators.ReplayExActivity" />
        <activity android:name=".ui.operators.ConcatExActivity" />
        <activity android:name=".ui.operators.MergeExActivity" />
        <activity android:name=".ui.operators.DeferExActivity" />
        <activity android:name=".ui.operators.DistinctExActivity" />
        <activity android:name=".ui.operators.LastOperatorExActivity" />

    </application>

</manifest>